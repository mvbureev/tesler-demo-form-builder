{
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es6",
    "sourceMap": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "skipLibCheck": true,
    "strict": true,
    // TODO: Fix errors which not match this rule
    "strictNullChecks": false,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "incremental": true,
    "paths": {
      "@rjsf/material-ui/*": ["./node_modules/@rjsf/material-ui/dist/*"]
    }
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./node_modules/"
  ]
}
