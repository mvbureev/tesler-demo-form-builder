{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "craco start",
    "build": "react-scripts build && npm run rename",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "gen:schema": "node ./node_modules/@tesler-ui/schema/bin/build-schema",
    "rename": "node -e \"require('fs').rename('build/index.html', 'build/index.ftl', function(err) { if (err) console.log(err); console.log('index.ftl formed') })\""
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.6",
    "@ant-design/icons": "^4.7.0",
    "@designable/core": "^1.0.0-beta.45",
    "@designable/formily-setters": "^1.0.0-beta.45",
    "@designable/formily-transformer": "^1.0.0-beta.45",
    "@designable/react": "^1.0.0-beta.45",
    "@formily/json-schema": "^2.1.9",
    "@formily/path": "^2.1.5",
    "@formily/reactive-react": "^2.1.5",
    "@tesler-ui/core": "^1.33.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^10.4.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^12.20.15",
    "@types/react": "^16.14.10",
    "@types/react-dom": "^16.9.13",
    "@types/react-redux": "^7.1.16",
    "antd": "^4.21.2",
    "axios": "0.19.0",
    "classnames": "^2.3.1",
    "keycloak-js": "^14.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-scripts": "^4.0.3",
    "redux": "^4.1.0",
    "redux-observable": "^0.14.1",
    "rxjs": "^5.5.12",
    "typescript": "^3.9.10"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier",
      "@typescript-eslint"
    ],
    "rules": {
      "curly": "error",
      "prettier/prettier": "error",
      "@typescript-eslint/no-var-requires": 0,
      "@typescript-eslint/no-explicit-any": "off",
      "react/display-name": "off"
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2020,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "homepage": ".",
  "devDependencies": {
    "@craco/craco": "^6.4.3",
    "@formily/antd": "^2.1.5",
    "@formily/core": "^2.1.5",
    "@formily/react": "^2.1.5",
    "@formily/reactive": "^2.1.5",
    "@formily/shared": "^2.1.5",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "http-proxy-middleware": "^2.0.1",
    "prettier": "^2.3.2",
    "source-map-loader": "0.2.4"
  }
}
